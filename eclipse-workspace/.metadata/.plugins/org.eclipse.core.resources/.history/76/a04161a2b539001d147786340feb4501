package intro;

public class recapDemo2 {

	public static void main(String[] args) {
		double [] myList = {1.2,1.3,4.3,5.6} ;
		for (double number: myList) {
			System.out.println(number);
				
			
		}
		double[] myList = { 1.2,1.3,4.3,5.6 } ;
		for (double number: myList) {
			double total=0;
			total = total  + number;
			System.out.println(number);
			
		}
		System.out.println("toplam=" + total);
	}

}
